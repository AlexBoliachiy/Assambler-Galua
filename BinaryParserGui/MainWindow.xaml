<Window x:Class="BinaryParserGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinaryParserGui"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="600">
    <Grid>
        <ScrollViewer HorizontalAlignment="Left" Height="759" Margin="411,10,0,0" VerticalAlignment="Top" Width="171">
            <TextBox x:Name="code" TextWrapping="Wrap" Text="" Height="759" Width="171"/>
        </ScrollViewer>
        <ScrollViewer HorizontalAlignment="Left" Height="759" VerticalAlignment="Top" Width="138">
            <TextBox x:Name="data" TextWrapping="Wrap" Text="" Height="768"/>
        </ScrollViewer>
        <Label x:Name="label1" Content="Data" HorizontalAlignment="Left" Margin="164,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Code" HorizontalAlignment="Left" Margin="337,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Выбрать код и получить вывод" Margin="164,105,196,0" VerticalAlignment="Top" Height="29" Click="button_Click"/>

    </Grid>
</Window>
